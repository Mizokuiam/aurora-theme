{
  "name": "aurora-theme-pro",
  "version": "1.1.0",
  "description": "A beautiful and modern theme for VS Code. Premium developer experience with 11 carefully crafted themes.",
  "contributes": {
    "themes": [
      {
        "label": "Aurora",
        "uiTheme": "vs-dark",
        "path": "./themes/aurora-dark-color-theme.json"
      }
    ]
  },
  "scripts": {
    "test": "mocha test/**/*.test.js",
    "build": "node scripts/build.js",
    "validate": "node scripts/validate-themes.js",
    "check-contrast": "node scripts/check-contrast.js",
    "build:vscode": "node scripts/build-vscode.js",
    "build:jetbrains": "node scripts/build-jetbrains.js",
    "build:sublime": "node scripts/build-sublime.js",
    "build:atom": "node scripts/build-atom.js",
    "build:marketing": "node scripts/build-marketing.js",
    "serve": "node scripts/serve.js",
    "generate-previews": "node scripts/create-preview-images.js"
  },
  "keywords": [
    "theme",
    "vscode",
    "jetbrains",
    "sublime",
    "atom"
  ],
  "author": "Mizokuiam",
  "license": "MIT",
  "devDependencies": {
    "archiver": "^7.0.1",
    "canvas": "^3.0.0",
    "clean-css": "^5.3.2",
    "fs-extra": "^11.1.0",
    "html-minifier": "^4.0.0",
    "jimp": "^1.6.0",
    "mocha": "^10.2.0",
    "plist": "^3.1.0",
    "sharp": "^0.33.5",
    "terser": "^5.19.2",
    "xml2js": "^0.6.2"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "publisher": "mizokuiam",
  "repository": {
    "type": "git",
    "url": "https://github.com/mizokuiam/aurora-theme"
  }
}
