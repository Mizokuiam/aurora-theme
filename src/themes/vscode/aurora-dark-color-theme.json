{
  "name": "Aurora",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1b26",
    "editor.foreground": "#c0caf5",
    "editor.selectionBackground": "#283457",
    "editor.selectionForeground": "#ffffff",
    "activityBar.background": "#1a1b26",
    "sideBar.background": "#1f2335",
    "titleBar.activeBackground": "#1a1b26",
    "statusBar.background": "#1a1b26",
    "editorGroupHeader.tabsBackground": "#1f2335",
    "tab.activeBackground": "#1a1b26",
    "tab.inactiveBackground": "#1f2335"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#9d7cd8"
      }
    },
    {
      "scope": ["string", "string.template"],
      "settings": {
        "foreground": "#9ece6a"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#565f89",
        "fontStyle": "italic"
      }
    }
  ]
}
